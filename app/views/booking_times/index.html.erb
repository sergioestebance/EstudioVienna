<h1>Listing booking_times</h1>

<table>
  <tr>
    <th>Booking</th>
    <th>Hora</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @booking_times.each do |booking_time| %>
  <tr>
    <td><%= booking_time.booking_id %></td>
    <td><%= booking_time.hora_id %></td>
    <td><%= link_to 'Show', booking_time %></td>
    <td><%= link_to 'Edit', edit_booking_time_path(booking_time) %></td>
    <td><%= link_to 'Destroy', booking_time, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

